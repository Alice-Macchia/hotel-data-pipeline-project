{
  "datasets": [
    {
      "name": "db0a128d",
      "displayName": "KPI 1: Daily Revenue",
      "queryLines": [
        "SELECT date, gross_revenue\r\n",
        "FROM catalog_progetto_finale.gold_schema_pf.kpi_daily_revenue_gold\r\n",
        "ORDER BY date ASC"
      ]
    },
    {
      "name": "ebc39dbb",
      "displayName": "KPI 2: Cancellation Rate by Source",
      "queryLines": [
        "SELECT source, cancellation_rate_pct\r\n",
        "FROM catalog_progetto_finale.gold_schema_pf.kpi_cancellation_rate_by_source_gold\r\n",
        "ORDER BY cancellation_rate_pct DESC"
      ]
    },
    {
      "name": "255c582c",
      "displayName": "KPI 3: Collection Rate",
      "queryLines": [
        "SELECT hotel_id, collection_rate * 100 AS collection_rate_percentage\r\n",
        "FROM catalog_progetto_finale.gold_schema_pf.kpi_collection_rate_by_hotel_gold\r\n",
        "ORDER BY hotel_id"
      ]
    },
    {
      "name": "49c81936",
      "displayName": "KPI 4: Overbooking Alerts",
      "queryLines": [
        "SELECT room_id, booking_id_1, booking_id_2, overlap_start, overlap_end\r\n",
        "FROM catalog_progetto_finale.gold_schema_pf.kpi_overbooking_alerts_gold"
      ]
    },
    {
      "name": "5e7b46f4",
      "displayName": "contatore KPI 4",
      "queryLines": [
        "SELECT COUNT(*) as total_overbookings FROM catalog_progetto_finale.gold_schema_pf.kpi_overbooking_alerts_gold"
      ]
    },
    {
      "name": "459c5800",
      "displayName": "KPI 5: Customer Value",
      "queryLines": [
        "SELECT first_name, last_name, bookings_count, revenue_sum, avg_ticket\r\n",
        "FROM catalog_progetto_finale.gold_schema_pf.kpi_customer_value_gold\r\n",
        "ORDER BY revenue_sum DESC\r\n",
        "LIMIT 20 -- Limita ai primi 20 per non appesantire la dashboard"
      ]
    },
    {
      "name": "b6577f75",
      "displayName": "Fatturato totale",
      "queryLines": [
        "-- Formatta il numero per renderlo più leggibile e aggiunge il simbolo dell'Euro\r\n",
        "SELECT CONCAT('€ ', FORMAT_NUMBER(SUM(gross_revenue), 0))\r\n",
        "FROM catalog_progetto_finale.gold_schema_pf.kpi_daily_revenue_gold"
      ]
    },
    {
      "name": "afc28fe5",
      "displayName": "Tasso di incasso medio",
      "queryLines": [
        "SELECT CONCAT(FORMAT_NUMBER(AVG(collection_rate) * 100, 1), ' %')\r\n",
        "FROM catalog_progetto_finale.gold_schema_pf.kpi_collection_rate_by_hotel_gold"
      ]
    },
    {
      "name": "6224056d",
      "displayName": "Prenotazioni totali",
      "queryLines": [
        "SELECT FORMAT_NUMBER(SUM(bookings_count), 0)\r\n",
        "FROM catalog_progetto_finale.gold_schema_pf.kpi_daily_revenue_gold"
      ]
    }
  ],
